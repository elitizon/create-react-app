{
  "name": "@elitizon/tailwind-react-app",
  "version": "1.0.0",
  "description": "A create-react-script template with tailwindcss and typsecript",
  "keywords": [
    "reactjs",
    "tailwindcss"
  ],
  "homepage": "https://github.com/elitizon/create-react-app/blob/master/README.md",
  "author": {
    "email": "contact@elitizon.com",
    "name": "@elitizon"
  },
  "repository": "git://github.com/elitizon/create-react-app.git",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.1.3",
    "@types/jest": "^26.0.12",
    "@types/node": "^14.6.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "chokidar": ">=3.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1",
    "tailwindcss": "^2.0.2",
    "twin.macro": "^2.0.8",
    "typescript": "^4.0.2"
  },
  "resolutions": {
    "chokidar": ">=3.4.0",
    "@testing-library/dom": ">=7.21.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "yarn test --coverage --watchAll=false --coverageDirectory=./output/coverage/jest",
    "eject": "react-scripts eject",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/main.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/main.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "autoprefixer": "^10.2.1",
    "postcss": "^8.2.3",
    "postcss-cli": "^8.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!<rootDir>/src/serviceWorker.ts",
      "!<rootDir>/src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "coverageReporters": [
      "text-summary",
      "text",
      "html",
      "clover"
    ],
    "snapshotSerializers": []
  }
}
